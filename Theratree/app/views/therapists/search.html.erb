<p id="notice"><%= notice %></p>

<h1>Great! Here are some therapists we think you should check out.</h1>

<!--to display the therapist-->
<div class="row-fluid">
  <div class="span10">
    <% puts @search_therapists %>
    <% @search_therapists.each do |therapist| %>
      <% therapist = @therapists.find_by therapist_id: therapist%>
      <div class="small-hero-unit">
        <h3><%= therapist.name %></h3>
        <!--<h3>@therapist.title</h3>-->
        <p><%= therapist.phone %></p>
        <p><%= link_to 'Show', therapist, id: therapist.therapist_id %></p>
      </div>
    <% end %>
  </div>
</div>