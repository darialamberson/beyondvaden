<body>

<div class="navbar navbar-inverse navbar-fixed-top">
  <div class="navbar-inner">
    <div class="container-fluid">
      <a class="brand" href="#">Beyond Vaden</a>
      <div class="nav-collapse collapse">
        <p class="navbar-text pull-right">
          Logged in as <a href="#" class="navbar-link">Username</a>
        </p>
        <ul class="nav">
          <li><a href="#">Home</a></li>
          <li class="active"><a href="#">Therapists</a></li>
          <li><a href="#search">Search</a></li>
        </ul>
      </div>
    </div>
  </div>
</div>

<div class="container-fluid">
  <div class="row-fluid">
    <div class="span3">
      <div class="well sidebar-nav">
        <ul class="nav nav-list">
          <li>
            <img src="img/jean.jpg" alt="..." class="img-rounded"></img>
          </li>
          <li class="nav-header">Quick Information</li>
          <li class="active"><a href="#">Link</a></li>
          <li><a href="#">Link</a></li>
          <li><a href="#">Link</a></li>
          <li><a href="#">Link</a></li>
          <li class="nav-header">Useful Links</li>
          <li><a href="#">Link</a></li>
          <li><a href="#">Link</a></li>
          <li><a href="#">Link</a></li>
          <li><a href="#">Link</a></li>
          <li><a href="#">Link</a></li>
          <li><a href="#">Link</a></li>
          <li class="nav-header">Rollin' With the Homiezzz</li>
          <li><a href="#">Link</a></li>
          <li><a href="#">Link</a></li>
          <li><a href="#">Link</a></li>
        </ul>
      </div>
    </div>
    <div class="span9">
      <div class="hero-unit">
        <h1>Jean Hamilton</h1>
        <h3><% @therapist.title %></h3>
        <p><% @therapist.description %></p>
      </div>
      <div class="row-fluid">
        <div class="span4">
          <h2>About</h2>
          <ul class="therapist-list">
            <li><b>Phone</b>: <% @therapist.phone_number %></li>
            <li><b>Qualifications</b>: <% @therapist.qualifications %></li>
            <li><b>Age:</b> <% @therapist.age %>></li>
            <li><b>Ethnicity:</b> <% @therapist.ethnicity %></li>
          </ul>
        </div>
        <div class="span4">
          <h2>Location</h2>
          <p><% @therapist.location %></p>
          <p><a class="btn" href="#">Show Map &raquo;</a></p>
        </div>
        <div class="span4">
          <h2>Specialties</h2>
          <ul class="therapist-list">
            <li><% @therapist.specialties %></li>
          </ul>
        </div>
      </div>
      <div class="row-fluid">
        <div class="span4">
          <h2>Treatment Approach</h2>
          <ul class="therapist-list">
            <li><% @therapist.treatment_orientation %></li>
            <li><% @therapist.modality %></li>
          </ul>
        </div>
        <div class="span4">
          <h2>Finances</h2>
          <ul class="therapist-list">
            <li><% @therapist.finances %></li>
          </ul>
        </div>
      </div>
    </div>
  </div>

  <hr>butts

  <footer>
    <p>&copy; Beyond Vaden 2016</p>
  </footer>

</div><!--/.fluid-container-->

